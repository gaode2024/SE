" 定义基本结构
TYPES: BEGIN OF ty_avatar_urls,
         48x48 TYPE string,
         24x24 TYPE string,
         16x16 TYPE string,
         32x32 TYPE string,
       END OF ty_avatar_urls.

TYPES: BEGIN OF ty_user,
         self          TYPE string,
         name          TYPE string,
         key           TYPE string,
         email_address TYPE string,
         avatar_urls   TYPE ty_avatar_urls,
         display_name  TYPE string,
         active        TYPE abap_bool,
         time_zone     TYPE string,
       END OF ty_user.

TYPES: BEGIN OF ty_status_category,
         self       TYPE string,
         id         TYPE i,
         key        TYPE string,
         color_name TYPE string,
         name       TYPE string,
       END OF ty_status_category.

TYPES: BEGIN OF ty_status,
         self           TYPE string,
         description    TYPE string,
         icon_url       TYPE string,
         name           TYPE string,
         id             TYPE string,
         status_category TYPE ty_status_category,
       END OF ty_status.

TYPES: BEGIN OF ty_priority,
         self     TYPE string,
         icon_url TYPE string,
         name     TYPE string,
         id       TYPE string,
       END OF ty_priority.

TYPES: BEGIN OF ty_issuetype,
         self        TYPE string,
         id          TYPE string,
         description TYPE string,
         icon_url    TYPE string,
         name        TYPE string,
         subtask     TYPE abap_bool,
         avatar_id   TYPE i,
       END OF ty_issuetype.

TYPES: BEGIN OF ty_fields,
         summary          TYPE string,
         issuetype        TYPE ty_issuetype,
         duedate          TYPE string,
         description      TYPE string,
         reporter         TYPE ty_user,
         customfield_10220 TYPE string,
         assignee         TYPE ty_user,
         priority         TYPE ty_priority,
         updated          TYPE string,
         labels           TYPE string_table,
         status           TYPE ty_status,
       END OF ty_fields.

TYPES: BEGIN OF ty_issue,
         expand        TYPE string,
         id            TYPE string,
         self          TYPE string,
         key           TYPE string,
         fields        TYPE ty_fields,
         renderedfields TYPE string,
       END OF ty_issue.

" 主响应结构
TYPES: BEGIN OF ty_jira_response,
         expand         TYPE string,
         start_at       TYPE i,
         max_results    TYPE i,
         total          TYPE i,
         issues         TYPE TABLE OF ty_issue WITH EMPTY KEY,
         warning_messages TYPE string_table,
         names          TYPE string,
         schema         TYPE string,
       END OF ty_jira_response.

" 定义内表和工作区
DATA: gt_jira_data TYPE ty_jira_response,
      gs_jira_data TYPE ty_jira_response.

DATA: gt_issues TYPE TABLE OF ty_issue,
      gs_issue  TYPE ty_issue.
